{
    "name": "san-docit",
    "description": "San document creater.",
    "author": "ecomfe",
    "private": true,
    "bin": {
        "docit": "bin/index.js"
    },
    "scripts": {
        "start": "yarn workspace san-docit start",
        "build": "yarn workspace san-docit build"
    },
    "dependencies": {
    },
    "workspaces": [
        "packages/*"
    ],
    "files": [
        "src",
        "bin",
        "build",
        "plugins",
        "packages",
        "index.ejs"
    ],
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ],
        "exclude": "node_modules"
    },
    "engines": {
        "node": ">=8.16"
    },
    "license": "MIT"
}
